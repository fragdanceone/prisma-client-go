datasource db {
  provider = "mysql"
  url      = env("__REPLACE__")
}

generator db {
  provider          = "go run github.com/fragdance1/prisma-client-go"
  output            = "."
  disableGoBinaries = true
  package           = "db"
  previewFeatures = ["filterJson"]
}

model User {
  id      String @id @default(cuid())
  json    Json
  jsonOpt Json?
}
